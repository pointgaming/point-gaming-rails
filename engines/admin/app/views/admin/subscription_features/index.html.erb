<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @subscription_features.each do |subscription_feature| %>
      <tr>
        <td><%= link_to subscription_feature.name, admin_subscription_feature_path(subscription_feature) %></td>
        <td>
          <%= link_to "Edit", edit_admin_subscription_feature_path(subscription_feature), class: "btn btn-mini" %>
          <%= link_to "Destroy", admin_subscription_feature_path(subscription_feature), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-mini btn-danger" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "New", new_admin_subscription_feature_path, class: "btn" %>
