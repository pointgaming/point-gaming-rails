<h1>Game Type</h1>

<dl class="dl-horizontal">
  <dt><strong>Name:</strong></dt>
  <dd><%= @game_type.name %></dd>
</dl>

<div class="form-actions">
  <%= link_to "Back", admin_game_types_path, class: "btn"  %>
  <%= link_to "Edit", edit_admin_game_type_path(@game_type), class: "btn" %>
  <%= link_to "Destroy", admin_game_type_path(@game_type), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-danger" %>
</div>
