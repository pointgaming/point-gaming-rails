<h1>Title</h1>

<dl class="dl-horizontal">
  <dt><strong>Title</strong></dt>
  <dd><%= @news.title %></dd>
  <dt><strong>Content:</strong></dt>
  <dd><%= @news.content %></dd>
</dl>

<div class="form-actions">
  <%= link_to "Back", admin_news_path, class: "btn"  %>
  <%= link_to "Edit", admin_edit_news_path(@news), class: "btn" %>
  <%= link_to "Destroy", admin_news_path(@news), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-danger" %>
</div>
