<h1>Subscription Type</h1>

<dl class="dl-horizontal">
  <dt><strong>Name:</strong></dt>
  <dd><%= @subscription_type.name %></dd>
</dl>

<div class="form-actions">
  <%= link_to "Back", admin_subscription_types_path, class: "btn"  %>
  <%= link_to "Edit", edit_admin_subscription_type_path(@subscription_type), class: "btn" %>
  <%= link_to "Destroy", admin_subscription_type_path(@subscription_type), method: "delete", data: { confirm: t(".confirm", default: t("helpers.links.confirm", default: "Are you sure?")) }, class: "btn btn-danger" %>
</div>
